{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-typescript",
      {
        "allowNamespaces": true,
        "allowDeclareFields": true,
        "onlyRemoveTypeImports": false
      }
    ]
  ]
}
